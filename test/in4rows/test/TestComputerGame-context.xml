<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="in4rowsFActory" class="in4rows.In4RowsFactory" />

	<bean id="testCallback" class="in4rows.test.tech.OnePlayerMovesTestCallback" />

	<bean id="assertCallbackBuilder" class="in4rows.test.tech.AssertEventCallbackBuilder" />

	<bean id="evtDispatcher" class="in4rows.event.EventDispatcher" />

	<bean id="strategyP1" class="in4rows.test.tech.DummyStrategy"
		name="s1" />
	<bean id="strategyP2" class="in4rows.test.tech.DummyStrategy"
		name="s2" />

	<bean id="player1" class="in4rows.test.tech.DummyComputerPlayer"
		name="p1">
		<constructor-arg type="java.lang.String" name="id"
			value="player 1" />
		<property name="strategy" ref="strategyP1" />
	</bean>
	<bean id="player2" class="in4rows.test.tech.DummyComputerPlayer"
		name="p2">
		<constructor-arg type="java.lang.String" name="id"
			value="player 2" />
		<property name="strategy" ref="strategyP2" />
	</bean>

</beans>