<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="testCallback" class="in4rows.test.tech.NotExecuteCallback" />
	<bean id="evtDispatcher" class="in4rows.event.EventDispatcher" />

	<bean id="in4rowsFActory" class="in4rows.In4RowsFactory" scope="singleton">
		<property name="dispatcher" ref="evtDispatcher" />
	</bean>
</beans>